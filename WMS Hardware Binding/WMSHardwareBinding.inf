[WMSHardwareBinding_Device.NTamd64.Services]
AddService = WMSHardwareBinding, %SPSVCINST_ASSOCSERVICE%, WMSHardwareBinding_Service_Inst
[WMSHardwareBinding_Service_Inst]
DisplayName    = %WMSHardwareBinding.SVCDESC%
ServiceType    = 1 
StartType      = 3   
ErrorControl   = 1   
ServiceBinary  = %12%\WMSHardwareBinding.sys
[WMSHardwareBinding_Device.NTamd64.Wdf]
KmdfService = WMSHardwareBinding, WMSHardwareBinding_wdfsect
[WMSHardwareBinding_wdfsect]
KmdfLibraryVersion = $KMDFVERSION$
[Strings]
SPSVCINST_ASSOCSERVICE = 0x00000002
ManufacturerName = "we-make-software.com"
DiskName = "WMSHardwareBinding Installation Disk"
WMSHardwareBinding.DeviceDesc = "WMSHardwareBinding"
WMSHardwareBinding.SVCDESC = "WMSHardwareBinding Service"